{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "installCommand": "bundle config set path vendor/bundle && bundle install",
  "buildCommand": "bash build.sh",
  "outputDirectory": "_site",
  "env": {
    "LANG": "en_US.UTF-8",
    "LC_ALL": "en_US.UTF-8",
    "LANGUAGE": "en_US.UTF-8",
    "JEKYLL_ENV": "production",
    "RUBYOPT": "-Eutf-8:utf-8"
  },
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://andy-wenk.de/cryptomarbel/:path*",
      "has": [
        {
          "type": "host",
          "value": "cryptomarbel.andy-wenk.de"
        }
      ],
      "permanent": false
    }
  ]
}