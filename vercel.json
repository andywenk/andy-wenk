{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "installCommand": "bundle config set path vendor/bundle && bundle install",
  "buildCommand": "bundle exec jekyll build -d _site --trace --profile",
  "outputDirectory": "_site",
  "env": {
    "LANG": "C.UTF-8",
    "LC_ALL": "C.UTF-8",
    "JEKYLL_ENV": "production",
    "RUBYOPT": "-Eutf-8:utf-8"
  }
}